{
  "name": "web-scraping-tutorial",
  "version": "1.0.0",
  "description": "web scraper that will scrape  'https://www.racingpost.com/tipping/naps-table' for daily naps",
  "main": "rpNapsScraper.js",
  "dependencies": {
    "async-require": "^1.2.2",
    "cheerio": "^1.0.0-rc.3",
    "json-2-csv": "^3.6.1",
    "moment": "^2.24.0",
    "puppeteer": "^2.1.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {},
  "scripts": {
    "naps": "node src/napsScraper.js",
    "forecasts": "node src/racecardScraper.js",
    "selections": "npm run naps && npm run forecasts"
  },
  "bin": {
    "naps": "./racingPostNapsScraper.js",
    "forecasts": "./racingPostRacecardScraper.js",
    "selections": "./racingPostNapsScraper.js && ./racingPostRacecardScraper"
  },
  "author": "Evan Blair",
